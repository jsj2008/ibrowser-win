{
    "name":"ibrowser",
    "version":"0.1.2",
    "manifest_version":2,
    "update_url":"http://liang8305.github.com/ibrowser/updates.html",
    "offline_enabled":true,
    "permissions":[
        "notifications",
        "alarms",
        "*://*/.ipa",
        "tabs",
        "webRequest",
        "webRequestBlocking"
    ],
    "web_accessible_resources":[
        "icon.jpeg"
    ],
    "plugins":[
        {
            "path":"npibrowser.dll","public":true
        }
    ],
    "browser_action":{
        "default_title":"ibrowser",
        "default_icon":"icon.jpeg",
        "default_popup":"popup.html"
    },
    "background":{
        "page":"background.html",
        "persistent":true
    }

}
